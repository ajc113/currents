                <%= f.select :species, all_species.map{ |species| [species.name, species.id] }, { prompt: "Select Species" }, class: "form-control m-b", style: "width: 100% !important", id: "species_select" %>
                <% end %>
            </div>
    
            <div class="col-lg-6">
              <%= form_for(:states_filter) do |f| %>
              <%= f.label :location %>
              
              <%= f.select :state, State.sorted.pluck(:name), { selected: current_user.state.name }, class: "form-control", id: "state_select", style: "width: 100% !important" %>
              <% end %>
            </div>
          </div>

      </div>
    </div>    
  </div>
  <br>
  <div id="locdetails" style="display:none;">
  </div>
  <div style="height: 900px; width: 100%;">
    <div id="map" class="gmap">
    </div>
  </div>
</div>


<script async defer src="https://maps.googleapis.com/maps/api/js?key=<%= ENV["google_map_api_key"]%>&callback=initMap"></script>
